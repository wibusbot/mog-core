{
  "version": 2,
  "installCommand": "pnpm install",
  "outputDirectory": "out/core",
  "buildCommand": "pnpm build && pnpm bundle",
  "framework": null,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
