{
  "version": 2,
  "installCommand": "pnpm install",
  "outputDirectory": "out",
  "buildCommand": "pnpm build && pnpm bundle",
  "framework": null,
  "functions": {
    "out/core/index.js": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "out/core/**/*.js"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "out/core/index.js"
    }
  ]
}
