{
  "version": 2,
  "installCommand": "pnpm install",
  "outputDirectory": "out",
  "buildCommand": "pnpm build && pnpm bundle:core",
  "framework": null,
  "builds": [
    {
      "src": "core/**/*.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "core/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/core/index.js"
    }
  ]
}
