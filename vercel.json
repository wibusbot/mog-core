{
  "version": 2,
  "installCommand": "pnpm install",
  "outputDirectory": "out",
  "buildCommand": "pnpm build && pnpm bundle",
  "builds": [
    {
      "src": "out/core/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "out/core/index.js"
    }
  ]
}
